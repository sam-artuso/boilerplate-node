{
  "name": "boilerplate-node",
  "version": "0.1.0",
  "description": "A bare bones boilerplate project for Node.js apps and NPM modules.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/",
    "build": "babel src/ -d dist/",
    "build:watch": "babel --watch src/ -d dist/",
    "test": "mocha --compilers js:babel-register \"test/**/*.js\"",
    "clean": "rm -rf ./dist",
    "eslint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samuele-artuso/boilerplate-node.git"
  },
  "bugs": {
    "url": "https://github.com/samuele-artuso/boilerplate-node/issues"
  },
  "keywords": [
    "node.js",
    "boilerplate"
  ],
  "author": "Sam Artuso <sam@highoctanedev.co.uk>",
  "license": "MIT",
  "directories": {
    "bin": "./bin",
    "lib": "./src/lib",
    "test": "./test"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "chai": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-mocha": "^4.9.0",
    "mocha": "^3.4.2"
  },
  "dependencies": {}
}
