extends:
  - airbnb-base
  - plugin:jest/recommended
plugins:
  - import
  - jest
rules:
  semi:
    - error
    - never
  max-len:
    - error
    - 120
  import/no-unresolved:
    - error
    - ignore:
      - "\\~"
  import/extensions:
    - error
    - js: never
  arrow-body-style:
    - 0
env:
  node: true
  jest/globals: true
